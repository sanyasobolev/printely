<h1>Редактирование</h1>
<%= error_messages_for :paper_size %>
<%= form_for :size, :url => lists_paper_size_path(@size), :html => {:method => :put} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
    <div class="submit_area">
      <%= submit_tag 'Сохранить', :class => 'submit' %>
      <%= link_to 'Назад', lists_paper_sizes_path, :class => 'button_style' %>
   </div>
  
<% end %>



