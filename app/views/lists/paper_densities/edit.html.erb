<h1><%= @title %></h1>
<div class="form_shape">
	<%= form_for :density, :url => lists_paper_density_path(@density), :html => {:method => :put} do |f| %>
	  <%= f.error_messages %>
	  <%= render :partial => 'form', :locals => {:f => f} %>
	    <div class="submit_area">
	      <%= submit_tag 'Сохранить', :class => 'submit' %>
	      <%= link_to 'Назад', lists_paper_densities_path, :class => 'button_style' %>
	   </div>
	  
	<% end %>
</div>



