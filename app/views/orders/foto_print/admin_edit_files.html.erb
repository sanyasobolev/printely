<% content_for :head do %>
	<%= javascript_include_tag "orders/deliveryfn" %>
	<%= javascript_include_tag "orders/uploadfn" %>
	<%= javascript_include_tag "orders/foto_print_order" %>
<% end %>
<h2><%= @title %></h2>

<% key = Rails.application.config.session_options[:key] %>

<%= content_for :scripts do %>
  <script type="text/javascript">
    var upload_params = {
      '<%= key %>' : '<%= cookies[key] %>',
      '<%= request_forgery_protection_token %>' : '<%= form_authenticity_token %>',
      '_http_accept': 'application/javascript'
    };	

  </script>
<% end %>

<%= form_for @order, :url => order_path(@order), :html => { :method => :put, :id => @order.id, :class => "admin_foto_print_order"} do |order| -%>
  <h2 class="orange">Загруженные файлы</h2>
  
  	<%= render :partial => 'orders/foto_print/filelist', :locals => {:order => @order} %>
  
    <%= render :partial => 'orders/share/total_and_submit', :locals => {:order => order, :current_order => @order}  %>	

<% end -%>
  <%= render :partial => 'orders/share/cancel', :locals => {:current_order => @order}  %>	

 