<tr>
  <td><%=h (order.id) %></td>
  <td><%=h (Russian::strftime(order.created_at, "%R, %d %B %Y года")) %></td>
  <td><%=h (order.status) %></td>
  <td><%=h (number_to_currency(order.cost, :unit => "коп.", :separator => " руб. ")) %></td>
  <td><%=h (order.manager_comment) %></td>
  <td><%= link_to image_tag("icons/details_view_order.png", :border => "0"), order_path(order) %></td>
</tr>