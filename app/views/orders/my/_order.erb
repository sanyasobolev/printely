<tr class="grey_on_hover center">
  <td><%=h (order.id) %></td>
  <td><%=h (Russian::strftime(order.created_at, "%R, %d %B %Y года")) %></td>
  <td><%=h (order.order_status.title) %></td>
  <td><%=h (read_cost(order)) %></td>
  <td><%=h (order.manager_comment) %></td>
  <td><%=h (order.order_type.description) %></td>
  <td><%= link_to image_tag("orders/view_details.png", :border => "0"), order_path(order) %></td>
</tr>