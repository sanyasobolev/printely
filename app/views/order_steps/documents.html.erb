<%= render :partial => 'orders/share/title', :locals => {:title => @title } %>

<%= tutorial_progress_bar %>
<div class="fl_clear"></div>

<%= render :partial => 'orders/share/js_upload_vars', :locals => {:upload_button => @upload_button, :order => @order } %>
<%= render :partial => 'orders/share/form_header', :locals => {:form_header_class => @form_header_class, :form_header => @form_header } %>

<%= form_for @order, url: wizard_path, :html => { :method => :put, :id => @order.id} do |order| -%>
  	<%= render :partial => @partial, :locals => {:order => @order} %>	
  	<%= render :partial => 'orders/share/documents_price', :locals => {:order => @order} %>	
    <%= render :partial => 'orders/share/submit', :locals => {:order => order, :title => 'Перейти к доставке'} %>	 
<% end -%>

<div class="fl_clear"></div>
