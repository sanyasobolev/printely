<h1>Редактирование</h1>
<%= form_for :color, :url => admin_lists_print_color_path(@color), :html => {:method => :put} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>

		<div class="submit_area">
			<%= submit_tag 'Сохранить', :class => 'submit' %>
		</div> 
	<% end -%>
	<%= render :partial => 'share/cancel', :locals => {:path => admin_lists_print_colors_path}  %>




