<h1>Создание</h1>
<div class="form_shape">
	<%= form_for :order_type, :url => admin_lists_order_types_path do |f| %>
	  <%= f.error_messages %>
	  <%= render :partial => 'form', :locals => {:f => f} %>

		<div class="submit_area">
			<%= submit_tag 'Сохранить', :class => 'submit' %>
		</div> 
	<% end -%>
	<%= render :partial => 'share/cancel', :locals => {:path => admin_lists_order_types_path}  %>

</div>
 
