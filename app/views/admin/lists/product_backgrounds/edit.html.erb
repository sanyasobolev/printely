<h1>Редактирование</h1>
<div class="form_shape">
	<%= form_for :productbg, :url => admin_lists_product_background_path(@productbg), :html => {:method => :put, :id => 'edit_product_background', :multipart => true} do |f| %>
	  <%= f.error_messages %>
	  <%= render :partial => 'form', :locals => {:f => f, :productbg => @productbg} %>
		<div class="submit_area">
			<%= submit_tag 'Сохранить', :class => 'submit' %>
		</div> 
	<% end -%>
	<%= render :partial => 'share/cancel', :locals => {:path => admin_lists_product_backgrounds_path}  %>

</div>


