<h1>Создание</h1>
<div class="form_shape">
<%= form_for :operation, :url => admin_lists_pre_print_operations_path do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  
		<div class="submit_area">
			<%= submit_tag 'Сохранить', :class => 'submit' %>
		</div> 
	<% end -%>
	<%= render :partial => 'share/cancel', :locals => {:path => admin_lists_pre_print_operations_path}  %>

</div>
 
