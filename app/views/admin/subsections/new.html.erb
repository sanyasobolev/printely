<%= error_messages_for :subsection %>
<h2>Создание нового подраздела</h2>
<div class="form_shape">
    <%= form_for(:subsection, :url => admin_subsections_path) do |f| -%>

      <%= render :partial => 'form', :locals => {:f => f} %>

		<div class="submit_area">
			<%= submit_tag 'Сохранить', :class => 'submit' %>
		</div> 
	<% end -%>
	<%= render :partial => 'share/cancel', :locals => {:path => admin_subsections_path}  %>

</div>
