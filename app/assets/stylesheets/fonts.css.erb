@font-face {
    font-family: Arial;
    src: url('arial.eot');
    src: url('arial.eot?#iefix') format('embedded-opentype'),
         url('arial.woff') format('woff'),
         url('arial.ttf') format('truetype'),
         url('arial.svg#arialregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

